void setup()
{
  irrecv.enableIRIn();
  Serial.begin(115200, SERIAL_8N1, SERIAL_TX_ONLY);
  Serial.println();
  Serial.println("setup");
  display.init(115200); // enable diagnostic output on Serial
  Serial.println("setup done");
  display.setRotation(3);
  display.fillScreen(GxEPD_WHITE);
  display.setFont(&FreeSerif9pt7b); //FreeSans9pt7b
  display.setTextColor(GxEPD_BLACK);
  display.setCursor(17, 47);
  display.print("Gel de Bano y Ducha");
    display.setCursor(212, 115);
     display.setTextSize(2);
  display.print("3,75");
  display.update();
    display.drawBitmap(0, 0, gImage_negro, 298, 130, GxEPD_BLACK);
  display.update();
  display.drawBitmap(0, 0, gImage_rojo, 298, 130, GxEPD_RED);
  display.update();
  delay(5000);

  
}

void loop()
{

  if (irrecv.decode(&results)) {

    inChar = results.value;

    if (inChar == '&')
    {
      inputString = " ";
    }
    inputString += inChar;

    irrecv.resume();  // Receive the next value
  }


  if (inChar == '-') {
    Serial.println(inputString);
    imprimir();
    inputString = "";
    inChar = '0';
//ESP.deepSleep(0, WAKE_RF_DISABLED);
  }

}
void imprimir() {
  display.fillScreen(GxEPD_WHITE);
  display.setFont(&FreeMonoBold12pt7b);
  display.setTextColor(GxEPD_BLACK);
  display.setCursor(50, 50);
  display.print(inputString);
  display.update();
  delay(5000);

}

